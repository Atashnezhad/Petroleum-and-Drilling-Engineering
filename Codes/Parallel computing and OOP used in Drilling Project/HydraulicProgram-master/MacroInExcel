Private Sub Label34_Click()
fileSaveName = Application.GetSaveAsFilename(InitialFileName:=ActiveWorkbook.Path, FileFilter:="Excel Files (*.xls), *.xls")
    If fileSaveName <> False Then
        msg = "A file named '" + fileSaveName + "' already exists in this location. Do you want to replace it?"
        
        'Define name and directory to save active workbook to
        'check for existing file
        ExistFile = Dir(fileSaveName)
        If ExistFile <> "" Then
            If MsgBox(msg, vbQuestion + vbYesNo) = vbYes Then
                'OK Chosen, replace following line with desired code
                ActiveWorkbook.SaveCopyAs Filename:=fileSaveName
            Else
                'Cancel chosen, replace following line with desired code
            End If
                'If file does not exist, save file
        Else
            ActiveWorkbook.SaveAs Filename:=fileSaveName
        End If
    End If

End Sub

Private Sub Label34_MouseMove(ByVal Button As Integer, ByVal Shift As Integer, ByVal X As Single, ByVal Y As Single)
Label26.Caption = "Save as"
Label34.Font.Bold = True

End Sub

Private Sub Label35_Click()
    ActiveSheet.PageSetup.PrintArea = _
        "$A$1:$F$29,$A$31:$E$61,$A$63:$F$91,$A$93:$G$123,$A$125:$F$153,$A$155:$G$185,$A$187:$F$215,$A$217:$G$247,$A$249:$F$277,$A$279:$G$309,$A$311:$F$339,$A$341:$G$371"

    Application.Dialogs(xlDialogPrint).Show

End Sub

